pkgname=("tvheadend_kr-freq")
pkgver=0.1
pkgrel=1
pkgdesc="tvheadend korean frequencies"
arch=("any")
url="https://tvheadend.org/"
license=("custom")
depends=("tvheadend")
source=("tvheadend_kr-freq.zip")
md5sums=("bc56c1d17197623eceed42b3f38e9ac5")

package() {
	cd "${pkgdir}"

	install -dm0755 "${pkgdir}"/usr/share/tvheadend/data/dvb-scan/atsc
	install -Dm0644 "${srcdir}"/kr-*-frequencies-* "${pkgdir}"/usr/share/tvheadend/data/dvb-scan/atsc/
}

