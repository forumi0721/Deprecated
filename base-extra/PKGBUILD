url="http://localhost/"
license=("Other")
depends=(
  "abs"
  "arch-install-scripts"
  "bash-completion"
  "btrfs-progs"
  "cpio"
  "dosfstools"
  "f2fs-tools"
  "mc"
  "net-tools"
  "pkgfile"
  "unzip"
  "vim"
  "wget"
  "zip"
)
if [ "${CARCH}" = "x86_64" ] || [ "${CARCH}" = "x86" ]; then
  depends+=(
    "gptdisk"
  )
fi

package() {
  cd "${pkgdir}"
}

