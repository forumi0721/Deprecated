pkgname=net-compat
pkgver=0.1
pkgrel=1
pkgdesc="net-tools"
arch=('armv6h' "arm")
license=("gpl")
url="http://sourceforge.net/projects/net-tools/"
depends=("glibc")
source=("ifconfig-${arch}"
	"route-${arch}")
md5sums=("SKIP"
	"SKIP")

package() {
	cd ${srcdir}
	install -d ${pkgdir}/sbin
	install -m755 ifconfig-${arch} ${pkgdir}/sbin/ifconfig
	install -m755 route-${arch} ${pkgdir}/sbin/route
}
