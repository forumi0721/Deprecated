# Maintainer: StoneCold <forumi0721[at]gmail[dot]com>

pkgname=("thunar-trash")
pkgver=0.1
pkgrel=1
pkgdesc="Thuar Trash/network fix"
arch=("any")
url="https://wiki.archlinux.org/index.php/Thunar#Trash.2Fnetwork_icons_disappear_randomly"
license=("Other")
depends=("thunar")
source=("Thunar")
md5sums=("SKIP")

package() {
	cd "${srcdir}"
	install -dm0755 "${pkgdir}"/usr/local/bin
	install -Dm0755 Thunar "${pkgdir}"/usr/local/bin/
}

